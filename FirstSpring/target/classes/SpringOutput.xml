<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="outputHelper" class="com.gzy.springtest.loosely_coupled_test01.OutputHelper">
        <property name="outputGenerator" ref="jsonOutputGenerator"/>
    </bean>

    <bean id="csvOutputGenerator" class="com.gzy.springtest.loosely_coupled_test01.CsvOutputGenerator"></bean>
    <bean id="jsonOutputGenerator" class="com.gzy.springtest.loosely_coupled_test01.JsonOutputGenerator"></bean>
</beans>